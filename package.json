{
  "name": "diavaia-backend",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "db:export": "node exportDb.js",
    "db:refresh": "node importDb.js",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:fresh": "pnpm db:refresh && pnpm dev",
    "npm:dev:fresh": "npm run db:refresh && npm run dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.33.0",
    "@payloadcms/db-postgres": "^3.33.0",
    "@payloadcms/next": "3.33.0",
    "@payloadcms/payload-cloud": "3.33.0",
    "@payloadcms/richtext-lexical": "3.33.0",
    "authorizenet": "^1.0.10",
    "axios": "^1.9.0",
    "basic-ftp": "^5.0.5",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "duckdb": "^1.3.0",
    "fast-csv": "^5.0.2",
    "ftp": "^0.3.10",
    "graphql": "^16.8.1",
    "mongodb": "^6.16.0",
    "multer": "^2.0.0",
    "next": "15.3.0",
    "node-cron": "^4.0.7",
    "payload": "3.33.0",
    "pg": "^8.16.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "redis": "^5.1.1",
    "sharp": "0.32.6",
    "shippo": "^2.15.0",
    "stream-transform": "^3.3.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/authorizenet": "^1.0.2",
    "@types/ftp": "^0.3.36",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.15.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "dotenv": "^16.5.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
